{
    "name": "@selling-partner/api-models",
    "version": "0.1.0",
    "description": "Workspace package that consolidates Amazon Selling Partner API models into a single merged schema.",
    "type": "module",
    "private": true,
    "license": "MIT",
    "packageManager": "bun@1.2.23",
    "engines": {
        "bun": ">=1.2.0",
        "node": ">=22.0.0"
    },
    "scripts": {
        "models:update": "./scripts/pull.sh",
