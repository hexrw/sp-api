{
    "name": "@selling-partner-api/models",
    "version": "0.2.0",
    "description": "Workspace package that consolidates Amazon Selling Partner API models into a single merged schema.",
    "type": "module",
    "license": "MIT",
    "packageManager": "bun@1.2.23",
    "repository": {
        "type": "git",
        "url": "https://github.com/selling-partner-api/selling-partner-api"
    },
    "engines": {
        "bun": ">=1.2.0",
        "node": ">=22.0.0"
    },
    "files": [
        "README.md",
        "package.json",
        "src",
        "scripts"
    ],
    "publishConfig": {
        "access": "public",
        "registry": "https://registry.npmjs.org/"
    },
    "scripts": {
        "models:update": "./scripts/pull.sh",
        "build": "bun run scripts/build.ts"
    },
    "exports": {
        ".": {
            "types": "./src/paths.ts",
            "import": "./src/paths.ts"
        },
        "./paths": {
            "types": "./src/paths.ts",
            "import": "./src/paths.ts"
        },
        "./merged.json": "./src/merged.json"
    },
    "devDependencies": {
        "bun-types": "latest",
        "@types/swagger2openapi": "^7.0.4",
        "openapi-typescript": "^7.8.0",
        "swagger2openapi": "^7.0.8"
    },
    "peerDependencies": {
        "typescript": "^5.0.0"
    },
    "dependencies": {
        "atlassian-openapi": "^1.0.21",
        "openapi-merge": "^1.3.3"
    }
}