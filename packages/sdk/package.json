{
    "name": "@selling-partner/sdk",
    "version": "2.0.0-beta.0",
    "description": "Amazon Selling Partner API JavaScript client/SDK with full type coverage.",
    "type": "module",
    "author": "Petr Kolonicz <github.com/hexrw>",
    "license": "MIT",
    "packageManager": "bun@1.2.23",
    "engines": {
        "node": ">=22.0.0",
        "bun": ">=1.2.0"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/hexrw/sp-api.git",
        "directory": "packages/sdk"
    },
    "homepage": "https://github.com/hexrw/sp-api/tree/main/packages/sdk#readme",
    "main": "./dist/index.js",
    "module": "./dist/index.js",
    "types": "./dist/index.d.ts",
    "files": [
        "dist",
        "README.md",
        "package.json"
    ],
    "sideEffects": false,
    "scripts": {
        "clean": "rm -rf dist",
        "generate:limits": "bun run scripts/generate-limits.ts",
        "build": "bun run clean && bun run generate:limits && tsc --project tsconfig.build.json",
        "typecheck": "tsc --noEmit",
        "test": "bun test"
    },
    "exports": {
        ".": {
            "types": "./dist/index.d.ts",
            "import": "./dist/index.js"
        },
        "./*": {
            "types": "./dist/*.d.ts",
            "import": "./dist/*.js"
        }
    },
    "dependencies": {
        "@selling-partner/api-models": "workspace:*",
        "limiter": "^3.0.0",
        "openapi-fetch": "^0.14.0"
    },
    "devDependencies": {
        "@types/node": "^24.7.0",
        "json-schema-to-typescript": "^15.0.3",
        "openapi-typescript": "^7.8.0",
        "ts-to-zod": "^3.7.0",
        "typescript": "^5.6.2"
    },
    "peerDependencies": {
        "typescript": ">=5.0.0"
    },
    "publishConfig": {
        "access": "public"
    }
}
