{
    "name": "@selling-partner-api/field-guide",
    "type": "module",
    "private": true,
    "packageManager": "bun@1.2.23",
    "workspaces": [
        "packages/*"
    ],
    "engines": {
        "bun": ">=1.2.0",
        "node": ">=22.0.0"
    },
    "scripts": {
        "lint": "bun run --cwd packages/sdk typecheck",
    "build": "bun run --cwd packages/models build && bun run --cwd packages/sdk build",
    "build:models": "bun run --cwd packages/models build",
        "build:sdk": "bun run --cwd packages/sdk build",
        "test": "bun test",
        "docs:dev": "vitepress dev docs",
        "docs:build": "vitepress build docs",
        "docs:preview": "vitepress preview docs",
        "prepare": "husky"
    },
    "devDependencies": {
        "@commitlint/cli": "^20.1.0",
    "@commitlint/config-conventional": "^20.0.0",
    "bun-types": "latest",
        "husky": "^9.1.7",
        "vitepress": "^1.6.4"
    },
    "peerDependencies": {
        "typescript": "^5"
    }
}